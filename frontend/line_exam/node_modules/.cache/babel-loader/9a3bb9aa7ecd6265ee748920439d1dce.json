{"ast":null,"code":"import healthCheckService from '@/services/HealthCheckService';\nexport default {\n  name: \"upload-component\",\n\n  data() {\n    return {\n      isDragging: false,\n      file: \"\"\n    };\n  },\n\n  methods: {\n    dragOver() {\n      this.isDragging = true;\n    },\n\n    dragLevave() {\n      this.isDragging = false;\n    },\n\n    async uploadFile() {\n      this.$emit('selectedFile');\n      let result = await healthCheckService.healthCheckUpload(this.file);\n      this.$emit('result', result);\n    },\n\n    requestUploadFile() {\n      this.file = this.$refs.file.files[0];\n      this.uploadFile();\n    },\n\n    dropFile(e) {\n      if (e.dataTransfer.files[0].type == \"text/csv\") {\n        this.file = e.dataTransfer.files[0];\n        this.uploadFile();\n      } else {\n        alert(\"file type not match\");\n      }\n    }\n\n  }\n};","map":{"version":3,"mappings":"AA8DA,OAAOA,kBAAP,MAA+B,+BAA/B;AACA,eAAe;EACbC,IAAI,EAAE,kBADO;;EAEbC,IAAI,GAAG;IACL,OAAO;MACLC,UAAU,EAAE,KADP;MAELC,IAAI,EAAE;IAFD,CAAP;EAID,CAPY;;EAQbC,OAAO,EAAE;IACPC,QAAQ,GAAG;MACT,KAAKH,UAAL,GAAkB,IAAlB;IACD,CAHM;;IAIPI,UAAU,GAAG;MACX,KAAKJ,UAAL,GAAkB,KAAlB;IACD,CANM;;IAQP,MAAMK,UAAN,GAAmB;MACjB,KAAKC,KAAL,CAAW,cAAX;MACA,IAAIC,MAAK,GAAI,MAAMV,kBAAkB,CAACW,iBAAnB,CAAqC,KAAKP,IAA1C,CAAnB;MACA,KAAKK,KAAL,CAAW,QAAX,EAAqBC,MAArB;IACD,CAZM;;IAcPE,iBAAiB,GAAG;MAClB,KAAKR,IAAL,GAAY,KAAKS,KAAL,CAAWT,IAAX,CAAgBU,KAAhB,CAAsB,CAAtB,CAAZ;MACA,KAAKN,UAAL;IACD,CAjBM;;IAmBPO,QAAQ,CAACC,CAAD,EAAI;MACV,IAAGA,CAAC,CAACC,YAAF,CAAeH,KAAf,CAAqB,CAArB,EAAwBI,IAAxB,IAAgC,UAAnC,EAA+C;QAC7C,KAAKd,IAAL,GAAYY,CAAC,CAACC,YAAF,CAAeH,KAAf,CAAqB,CAArB,CAAZ;QACA,KAAKN,UAAL;MACF,CAHA,MAGO;QACLW,KAAK,CAAC,qBAAD,CAAL;MACF;IACD;;EA1BM;AARI,CAAf","names":["healthCheckService","name","data","isDragging","file","methods","dragOver","dragLevave","uploadFile","$emit","result","healthCheckUpload","requestUploadFile","$refs","files","dropFile","e","dataTransfer","type","alert"],"sourceRoot":"","sources":["/Users/sayantina/Documents/project/Line_Exam/frontend/line-exam/src/components/uploadComponent.vue"],"sourcesContent":["<template>\n  <div class=\"container mx-auto my-5 w-1/3\">\n    <div class=\"text-2xl font-bold text-left py-2\">Websites Checker</div>\n    <div\n      class=\"\n        container\n        mx-auto\n        box-border\n        border-dashed border-4\n        rounded-xl\n        w-full\n        py-5\n        box-upload\n        text-gray-500\n      \"\n      @dragover.prevent=\"dragOver\"\n      @dragleave.prevent=\"dragLeave\"\n      @drop.prevent=\"dropFile($event)\"\n    >\n      <div\n        class=\"\n          bg-[url('./../../public/csv.png')] bg-no-repeat bg-center\n          w-28\n          h-28\n          mx-auto\n          bg-[length:100px_100px]\n        \"\n      ></div>\n      <div class=\"text-2xl font-bold py-2\">\n        Drag your .csv file here to start uploading\n      </div>\n      <div class=\"w-2/5 mx-auto\">\n        <div class=\"relative flex py-5 items-center\">\n          <div class=\"flex-grow border-t-4 border-gray-400\"></div>\n          <span class=\"flex-shrink mx-4 text-2xl font-bold text-gray-400\"\n            >OR</span\n          >\n          <div class=\"flex-grow border-t-4 border-gray-400\"></div>\n        </div>\n        <div\n          class=\"rounded-lg text-white text-2xl font-bold py-4 cursor-pointer\"\n          style=\"background-color: #6749f5\"\n          @click=\"$refs.file.click()\"\n        >\n          Browse File\n        </div>\n        <input\n          type=\"file\"\n          id=\"csv_file\"\n          name=\"csv_file\"\n          ref=\"file\"\n          style=\"display: none\"\n          accept=\".csv\"\n          hidden\n          @change=\"requestUploadFile\"\n        />\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport healthCheckService from '@/services/HealthCheckService';\nexport default {\n  name: \"upload-component\",\n  data() {\n    return {\n      isDragging: false,\n      file: \"\",\n    };\n  },\n  methods: {\n    dragOver() {\n      this.isDragging = true;\n    },\n    dragLevave() {\n      this.isDragging = false;\n    },\n\n    async uploadFile() {\n      this.$emit('selectedFile')\n      let result = await healthCheckService.healthCheckUpload(this.file)\n      this.$emit('result', result)\n    },\n\n    requestUploadFile() {\n      this.file = this.$refs.file.files[0]\n      this.uploadFile()\n    },\n\n    dropFile(e) {\n      if(e.dataTransfer.files[0].type == \"text/csv\") {\n        this.file = e.dataTransfer.files[0];\n        this.uploadFile()\n      } else { \n        alert(\"file type not match\")\n      }\n    },\n  },\n};\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped>\n.box-upload {\n  background: #f5f7fc;\n  border-color: #c7cddb;\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}